FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (-10e-3 -10e-3 0) (10e-3 10e-3 0.1e-3);

        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


